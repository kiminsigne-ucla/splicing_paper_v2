load_pkgs <- function(pkgs){
new_pkgs <- pkgs[!(pkgs %in% installed.packages()[, 'Package'])]
if(length(new_pkgs)) install.packages(new_pkgs)
for(pkg in pkgs){
suppressWarnings(suppressMessages(library(pkg, character.only = T)))
}
}
